<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

    <link href="~/Content/ClientDashSheet.css" rel="stylesheet" />





</head>
<body>

    <div class="navbar navbar-default navbar-fixed-top " style="background-color:#333">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>

            <p class="navbar-brand" style="font-family: Gabriola; font-size:280%; text-transform: uppercase;
color:rgba(194,112, 76, 1);
				margin-top:20px; ">DOCTOR QUICK</p>


        </div>
        <div class="collapse navbar-collapse" id="myNavbar">
            <ul class="nav navbar-nav navbar-right" style="margin-right: 5%; color: rgba(194, 112, 76, 1);">
                <li>
                    <a href="/Admin/AAppTable" style="font-size:150%; font-family:gabriola;" onmouseover="this.style.color = 'rgba(194, 112, 76, 1)'" onmouseout="    this.style.color = 'white'">
                        <span class="glyphicon glyphicon-pencil "></span>
                        Appointment
                    </a>
                </li>
                <li>
                    <a href="/Admin/ARegTable" style="font-size:150%; font-family:gabriola;" onmouseover="this.style.color = 'rgba(194, 112, 76, 1)'" onmouseout="    this.style.color = 'white'">
                        <span class="glyphicon glyphicon-book"></span>
                        Patient Registration
                    </a>
                </li>
                <li>
                    <a href="/Admin/SignUpTable" style="font-size:150%; font-family:gabriola;" onmouseover="this.style.color = 'rgba(194, 112, 76, 1)'" onmouseout="    this.style.color = 'white'">
                        <span class="glyphicon glyphicon-user glyph"></span>
                        Sign Up Table
                    </a>
                </li>

                <li>
                    <a href="/Main/SignIn" style="font-size:150%; font-family:gabriola;" onmouseover="this.style.color = 'rgba(194, 112, 76, 1)'" onmouseout="    this.style.color = 'white'">
                        <span class="glyphicon glyphicon-log-out glyph"></span>
                        Sign Out
                    </a>
                </li>

            </ul>



        </div>



    </div>

    


    <div class="main">
    </div>

    <script>
        function htmlbodyHeightUpdate() {
            var height3 = $(window).height()
            var height1 = $('.nav').height() + 50
            height2 = $('.main').height()
            if (height2 > height3) {
                $('html').height(Math.max(height1, height3, height2) + 10);
                $('body').height(Math.max(height1, height3, height2) + 10);
            }
            else {
                $('html').height(Math.max(height1, height3, height2));
                $('body').height(Math.max(height1, height3, height2));
            }

        }
        $(document).ready(function () {
            htmlbodyHeightUpdate()
            $(window).resize(function () {
                htmlbodyHeightUpdate()
            });
            $(window).scroll(function () {
                height2 = $('.main').height()
                htmlbodyHeightUpdate()
            });
        });</script>













    <div>
        @RenderBody()
    </div>
</body>
</html>
